{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Temperature",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "julabo_temperature_celsius",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "celsius",
          "min": -50,
          "max": 200,
          "thresholds": {
            "steps": [
              { "color": "blue", "value": null },
              { "color": "green", "value": 15 },
              { "color": "orange", "value": 30 },
              { "color": "red", "value": 50 }
            ]
          }
        }
      }
    },
    {
      "title": "Setpoint",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 0 },
      "targets": [
        {
          "expr": "julabo_setpoint_celsius",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "celsius",
          "min": -50,
          "max": 200
        }
      }
    },
    {
      "title": "Pump Running",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "julabo_pump_running",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "Stopped", "color": "red" } } },
            { "type": "value", "options": { "1": { "text": "Running", "color": "green" } } }
          ]
        }
      }
    },
    {
      "title": "Temperature vs Setpoint",
      "type": "timeseries",
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "julabo_temperature_celsius",
          "legendFormat": "Temperature",
          "refId": "A"
        },
        {
          "expr": "julabo_setpoint_celsius",
          "legendFormat": "Setpoint",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "celsius"
        }
      }
    },
    {
      "title": "Command Latency",
      "type": "histogram",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 18 },
      "targets": [
        {
          "expr": "rate(julabo_command_latency_seconds_sum[5m]) / rate(julabo_command_latency_seconds_count[5m])",
          "legendFormat": "Avg Latency",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "title": "Commands per Minute",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 18 },
      "targets": [
        {
          "expr": "rate(julabo_commands_total[1m]) * 60",
          "legendFormat": "{{ command }}",
          "refId": "A"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["julabo"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "title": "Julabo Chiller",
  "uid": "julabo-chiller"
}
